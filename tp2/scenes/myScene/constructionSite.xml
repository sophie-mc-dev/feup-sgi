<?xml version="1.0" encoding="UTF-16" standalone="yes"?>
<yaf>
    <!-- Global configuration block -->
    <globals background="0 0 0 1" ambient="1 1 1 1" />

    <fog color="1.0 1.0 1.0 1.0" near="30.0" far="100.0" />

    <skybox size="1000 1000 1000" 
        center="0 0 0" 
        emissive="1 1 1 0.5" 
        intensity="0.1" 
        front="scenes/myScene/textures/front.jpg" 
        back="scenes/myScene/textures/back.jpg" 
        up="scenes/myScene/textures/up.jpg" 
        down="scenes/myScene/textures/down.jpg"        
        right="scenes/myScene/textures/right.jpg" 
        left="scenes/myScene/textures/left.jpg" 
    />   
    
    <!-- Cameras block -->
    <cameras initial="Perspective">
        <perspective id="Perspective"
            angle="75"
            near="0.1"
            far="1000"
            location="10 10 3"
            target="0 0.0 0" />

        <orthogonal id="Left"
            near="0.1"
            far="1000"
            location="0 15 0"
            target="0.0 15 0.0"
            left="-5"
            right="5"
            bottom="-5"
            top="-5" />
        <orthogonal id="Top"
            near="0.1"
            far="1000"
            location="0 0 15"
            target="0.0 15 0.0"
            left="-5"
            right="5"
            bottom="-5"
            top="-5" />
        <orthogonal id="Front"
            near="0.1"
            far="1000"
            location="0 15 0"
            target="0.0 15 0.0"
            left="-5"
            right="5"
            bottom="-5"
            top="-5" />
        <orthogonal id="Back"
            near="0.1"
            far="1000"
            location="0 15 0"
            target="0.0 15 0.0"
            left="5"
            right="-5"
            bottom="-5"
            top="-5" />
        <orthogonal id="Right"
            near="0.1"
            far="1000"
            location="0 -15 0"
            target="0.0 -15 0.0"
            left="-5"
            right="5"
            bottom="-5"
